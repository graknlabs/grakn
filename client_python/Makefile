init:
	pip3 install pipenv
	pipenv install --dev


protobuf:
	# generate protobuf and grpc files for Keyspace
	pipenv run python -m grpc_tools.protoc -I../client-protocol/proto --python_out=./src/service/Keyspace/autogenerated --grpc_python_out=./src/service/Keyspace/autogenerated ../client-protocol/proto/Keyspace.proto
	# generate protobuf and grpc files for Session
	pipenv run python -m grpc_tools.protoc -I../client-protocol/proto --python_out=./src/service/Session/autogenerated --grpc_python_out=./src/service/Session/autogenerated ../client-protocol/proto/Concept.proto ../client-protocol/proto/Answer.proto ../client-protocol/proto/Session.proto



