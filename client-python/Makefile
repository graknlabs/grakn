init:
	pip3 install pipenv
	pipenv install --dev


protobuf:
	mkdir -p grakn/service/Keyspace/autogenerated
	mkdir -p grakn/service/Session/autogenerated
	# generate protobuf and grpc files for Keyspace
	python -m grpc_tools.protoc -I../client-protocol/proto --python_out=./grakn/service/Keyspace/autogenerated --grpc_python_out=./grakn/service/Keyspace/autogenerated ../client-protocol/proto/Keyspace.proto
	# generate protobuf and grpc files for Session
	python -m grpc_tools.protoc -I../client-protocol/proto --python_out=./grakn/service/Session/autogenerated --grpc_python_out=./grakn/service/Session/autogenerated ../client-protocol/proto/Concept.proto ../client-protocol/proto/Answer.proto ../client-protocol/proto/Session.proto



